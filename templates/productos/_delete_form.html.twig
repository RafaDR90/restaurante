<form method="post" action="{{ path('app_productos_delete', {'id': producto.id,'catId':catId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');" class="delete-form mt-2">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ producto.id) }}">
    <button type="submit" class="btn btn-danger">Eliminar</button>
</form>
